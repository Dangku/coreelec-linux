#include "sm1_s905x3_4g_1gbit.dts"

/{
	model = "Sinovoip Bananapi-M5";
	/delete-property/ amlogic-dt-id;
	coreelec-dt-id = "sm1_s905x3_bananapi_m5";

	/delete-node/ memory@00000000;
	/delete-node/ bt-dev;
	/delete-node/ wifi;
	/delete-node/ wifi_pwm_conf;
	/delete-node/ unifykey;

	auge_sound {
		/delete-property/ avout_mute-gpios;
	};

	cpu_opp_table0: cpu_opp_table0 {
		opp12 {
			opp-hz = /bits/ 64 <2016000000>;
			opp-microvolt = <1010000>;
		};
		opp13 {
			opp-hz = /bits/ 64 <2100000000>;
			opp-microvolt = <1030000>;
		};
	};

	cpus:cpus {
		cpu-map {
			cluster0:cluster0 {
				stock_freq = <1908000000>;
			};
		};
	};

	gpioleds {
		status = "okay";
		/delete-node/ sys_led;
		green_led {
			label = "green";
			gpios = <&gpio_ao GPIOAO_2 GPIO_ACTIVE_LOW>;
			default-state = "off";
			linux,default-trigger = "none";
		};

		blue_led {
			label = "blue";
			gpios = <&gpio_ao GPIOAO_11 GPIO_ACTIVE_LOW>;
			default-state = "off";
			linux,default-trigger = "heartbeat";
		};
	};

	gpio-reset {
		compatible = "linux,gpio-reset";
		usb_hub {
			gpios = <&gpio GPIOH_4 GPIO_ACTIVE_HIGH>;
			asserted-state = <0>;
			duration-ms = <100>;
		};
	};

	adc_keypad {
		compatible = "amlogic, adc_keypad";
		status = "okay";
		key_name = "power";
		key_num = <1>;
		io-channels = <&saradc SARADC_CH2>;
		io-channel-names = "key-chan-2";
		key_chan = <SARADC_CH2>;
		key_code = <116>;
		key_val = <0>; //val=voltage/1800mV*1023
		key_tolerance = <40>;
	};

	meson-remote {
		map = <&custom_maps>;
	};

	custom_maps:custom_maps {
		mapnum = <2>;
		map0 = <&map_0>;
		map1 = <&map_1>;
		map_0: map_0{
			mapname = "amlogic-remote-1";
			customcode = <0xfe01>;
			release_delay = <80>;
			size  = <13>;   /*keymap size*/
			keymap = <REMOTE_KEY(0x00, KEY_POWER)
				REMOTE_KEY(0x01, KEY_MENU)
				REMOTE_KEY(0x09, KEY_HOME)
				REMOTE_KEY(0x02, KEY_LEFT)
				REMOTE_KEY(0x0a, KEY_RIGHT)
				REMOTE_KEY(0x05, KEY_UP)
				REMOTE_KEY(0x04, KEY_DOWN)
				REMOTE_KEY(0x06, KEY_ENTER)
				REMOTE_KEY(0x03, KEY_VOLUMEDOWN)
				REMOTE_KEY(0x0b, KEY_VOLUMEUP)
				REMOTE_KEY(0x40, KEY_BACK)
				REMOTE_KEY(0x48, KEY_PLAYPAUSE)
				REMOTE_KEY(0x44, KEY_MUTE)>;
		};
		map_1: map_1{
			mapname = "amlogic-remote-2";
			customcode = <0xaf40>;
			release_delay = <80>;
			size  = <13>;
			keymap = <REMOTE_KEY(0x0e, KEY_POWER)
				REMOTE_KEY(0x07, KEY_MENU)
				REMOTE_KEY(0x02, KEY_HOME)
				REMOTE_KEY(0x01, KEY_LEFT)
				REMOTE_KEY(0x03, KEY_RIGHT)
				REMOTE_KEY(0x19, KEY_UP)
				REMOTE_KEY(0x05, KEY_DOWN)
				REMOTE_KEY(0x06, KEY_ENTER)
				REMOTE_KEY(0x12, KEY_VOLUMEDOWN)
				REMOTE_KEY(0x04, KEY_VOLUMEUP)
				REMOTE_KEY(0x0f, KEY_BACK)
				REMOTE_KEY(0x0d, KEY_PLAYPAUSE)
				REMOTE_KEY(0x11, KEY_MUTE)>;
		};
};

	efuse: efuse {
		compatible = "amlogic, efuse";
		read_cmd = <0x82000030>;
		write_cmd = <0x82000031>;
		get_max_cmd = <0x82000033>;
		key = <&efusekey>;
		status = "okay";
	};

	efusekey: efusekey {
		keynum = <1>;
		key0 = <&key_0>;

		key_0: key_0 {
			keyname = "uuid";
			offset = <0>;
			size = <32>;
		};
	};
};

&spicc0 {
	status = "disabled";

	/*
	 40 Pin Header : MOSI(GPIOX.8->19 Pin), MISO(GPIOX.9->21 Pin),
			 SPI0_CLK(GPIOX.11->23 Pin)
			 SPI_CE0(GPIOX.2->22 Pin), SPI_CE1(GPIOX.10->24 Pin)
	*/
	pinctrl-names = "default","gpio_periphs";
	pinctrl-0 = <&spicc0_pins_x>;
	num_chipselect = <2>;

	cs-gpios = <&gpio GPIOX_10 GPIO_ACTIVE_LOW>,
		   <&gpio GPIOH_6 GPIO_ACTIVE_LOW>;

	spidev@0 {
		status = "disabled";

		compatible = "linux,spidev";
		/* spi default max clock 100Mhz */
		spi-max-frequency = <100000000>;
		reg = <0>;
	};
};

&audiobus {
	tdmb: tdm@1 {
		pinctrl-0 = <&tdmb_mclk_ao &tdmout_b_ao>;
	};
};

&pinctrl_periphs {
	/delete-node/ spdifout;
	/delete-node/ spdifout_a_mute;
};

&pinctrl_aobus {
	spdifout: spdifout {
		mux {
			groups = "spdif_out_ao";
			function = "spdif_out_ao";
		};
	};

	tdmb_mclk_ao: mclk0_ao {
		mux { /* GPIOAO_9 */
			groups = "mclk0_ao";
			function = "mclk0_ao";
			drive-strength = <2>;
		};
	};

	tdmout_b_ao: tdmout_b {
		mux { /* GPIOAO_8, GPIOAO_7, GPIOAO_4 */
			groups = "tdmb_sclk_ao",
				"tdmb_fs_ao",
				"tdmb_dout0_ao";
			function = "tdmb_out_ao";
			drive-strength = <2>;
		};
	};
};

&sd_emmc_c {
	mmc-ddr-1_8v;
	mmc-hs200-1_8v;
	emmc {
		caps = "MMC_CAP_8_BIT_DATA",
			 "MMC_CAP_MMC_HIGHSPEED",
			 "MMC_CAP_SD_HIGHSPEED",
			 "MMC_CAP_NONREMOVABLE",
			 "MMC_CAP_HW_RESET",
			 "MMC_CAP_ERASE",
			 "MMC_CAP_CMD23";
		caps2 = "MMC_CAP2_HS200_1_8V_SDR",
			"MMC_CAP2_BROKEN_VOLTAGE";
		hw_reset = <&gpio BOOT_12 GPIO_ACTIVE_HIGH>;
	};
};

&sd_emmc_a {
	status = "disabled";
	/delete-node/ sdio;
};

&ethmac {
	rst_pin-gpios = <&gpio GPIOZ_15 GPIO_ACTIVE_LOW>;
};

&usb3_phy_v2 {
	otg = <1>;
};

&uart_A {
	status = "disabled";
};

&i2c2 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c2_master_pins1>;
	clock-frequency = <100000>;
};

&i2c3 {
	status = "disabled";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c3_master_pins2>;
	clock-frequency = <100000>;
};

&saradc {
	status = "okay";
};
